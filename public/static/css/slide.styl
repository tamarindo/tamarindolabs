#sequence 
    height 400px
    margin 40px auto 0;
    position relative /* required */
    width 100%
    text-align center
    color #fff

    > .canvas
    > .canvas li
    	height 100%
    	width 100%

    	li
    		position absolute
    		z-index 1
    		list-style none


/* Slide Welcome
========== */

.slide-welcome
	background url(../img/huge-bg-2.jpg) center center
	background-size cover
	height 100%
	text-transform uppercase
	h2 
		font-size 16px
		font-weight normal
		color cyan
		-webkit-animation-duration 1.5s
		-webkit-animation-delay 0.4s
		
	h1
		color #fff
		font-weight 100
		padding-top 140px
		font-size 52px
